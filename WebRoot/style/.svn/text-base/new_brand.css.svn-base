@charset "utf-8";
.brand_new { width:978px!important; height:96px!important; margin:0 auto; border:#cbdbe8 1px solid;}
.brand_new .brand_select { width:100%; height:22px; padding:19px 0px; float:left; background:url(/images/brand_newbg.gif) 0px 0px repeat-x;}
.brand_new .brand_select strong.n { width:88px; padding-left:18px; float:left; font:100 16px/22px 微软雅黑; color:#595959;}
.brand_new .brand_select .nsel { width:230px; float:left;}
.brand_new .brand_select .nsel b.n { width:32px; height:22px; line-height:22px; float:left; font-weight:normal; color:#333;}
.brand_new .brand_select .nsel span.sel { width:auto; float:left;}
.brand_new .brand_select .nsel span.sel select { width:158px; padding:2px; border:#bababa 1px solid;}
.brand_new .brand_select .btn { width:auto; float:left;}
.brand_new .brand_select .btn input { width:76px; height:22px; background:url(../images/brand_newbg.gif) 0px -61px no-repeat; border:none; color:#fff;}
.brand_new .brand_letter { width:100%; float:left; background-color:#f4f8fb;}
.brand_new .brand_letter strong.n { width:84px; padding-left:18px; line-height:34px; float:left; font-weight:normal; color:#9e9e9e;}
.brand_new .treeSubNavv1 { width:870px; height:36px;}
.brand_new .treeSubNavv1 ul li { margin-right:6px;}
.brand_new .treeSubNavv1 ul li.all a { width:32px; background-position:0px -100px;}
.brand_new .treeSubNavv1 ul li.all a:hover { background-position:-32px -100px;}
.hbTitle span.more { width:auto; height:28px; line-height:28px; float:right; padding:0px 16px; font-family:宋体;}
.hbTitle span.more a { color:#075aad!important;}
.hbTitle span.more a:hover { color:#ff6600!important;}
.hbTitle span.hotLinks { width:auto; float:right; padding-right:15px;}
.hbTitle span.hotLinks em.w { color:#878787; margin:0px 7px; color:#878787;}
.hbTitle span.hotLinks a { color:#878787!important;}
.hbTitle span.hotLinks a:hover { color:#ff6600!important;}
.Brand_newPart { margin-top:10px!important;}
.Brand_newPart .hbTitle,.Brand_newPart .hbTitle h2,.Brand_newPart .hbTitle span.more { height:32px; line-height:32px; cursor:default;}
.Brand_newPart .hbTitle { background-color:#e6ecf0; border-bottom:none;}
.Brand_newPart .hbTitle h2 { width:auto; padding:0px 42px; background:#fe6000;}
.Brand_newPart .hbTitle span.more { background:url(/images/brand_newbg.gif) 0px -83px no-repeat;}
.Brand_newPart .hbContain { border-top:#DEE3E7 1px solid;}
.sideGuide { width:98px; height:350px; position:fixed; right:0px; top:165px; z-index:300;}
* html .sideGuide { position:absolute; left:expression(eval(document.documentElement.scrollLeft+document.documentElement.clientWidth-this.offsetWidth)-(parseInt(this.currentStyle.marginLeft,10)||0)-(parseInt(this.currentStyle.marginRight,10)||0)); top:expression(eval(document.documentElement.scrollTop)+165)}

.sideGuide a { display:block; width:100%; height:28px; line-height:28px; border-bottom:#ffffff 1px solid; background-color:#e6ecf0; color:#333!important; text-align:center;}
.sideGuide a.selected,.sideGuide a:hover { background-color:#0855aa; color:#fff!important;}
.letterPoint { height:0px; clear:both; position:relative; line-height:0px; font-size:0px;}
.letterPoint a { display:block; width:1px; height:1px; position:absolute; left:0px; top:-100px;}

body.brandList .brandCondition,body.brandList .brandCondition .contain980 { width:757px;}
body.brandList .brandtopborder,body.brandList .brandbottomborder { width:755px;}
body.brandList .brandcontain925 { width:745px; padding-left:5px; padding-right:5px;}
body.brandList .blbg { margin:0px 4px 0px 5px;}
body.brandList .treev1 .root b { font-size:14px;}

.pllist { width:100%; *width:95%;  min-height:50px; height:auto!important; height:50px; overflow:visible; position:relative; padding-top:23px;border-bottom:#ccc 1px dotted; margin-right:1%; padding-left:8px;}
.pllist a.plfl { position:absolute; left:8px; height:40px; top:0px; margin:5px; font-size:14px; font-family:Verdana; font-weight:bold; color:#3355AC; float:none; padding:0px; width:auto; text-decoration:none;}
.pllist a { display:inline-block; margin:0px 5px; font-size:12px; line-height:20px; height:20px; overflow:hidden; float:none; padding:0px; width:auto; text-decoration:none;}





