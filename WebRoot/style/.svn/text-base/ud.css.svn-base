.dt_1 { border: #fff 1px solid; background-color: #f7f7f7; overflow: hidden; height:85px;}
a.Favorites { float: right; background-position: -85px top; width: 53px; height: 30px; border-left: 1px solid #e9e9e9; border-right: 0; text-align: center; padding-top: 50px; color: #999; cursor: pointer }
.dt_1 a.favcur { float: right; background-position: -256px top; width: 53px; height: 30px; border-left: 1px solid #e9e9e9; border-right: 0; text-align: center; padding-top: 50px; color: #999; cursor: pointer }
.dt_Title { width: 890px; float: left; padding: 0px 15px; }
.dt_Title h1, .dt_Title h2 { width: 800px; height: 24px; font: 700 20px/24px 微软雅黑; color: #484848; padding: 15px 0px 10px; float: left; white-space: nowrap; text-overflow: ellipsis; overflow: hidden; }
.dt_Title .inquiry { width: 500px; float: left; color: #666666; }
.dt_Title .inquiry strong.num { color: #e10000; margin: 0px 3px; }
.dt_Title .inquiry strong.btn { font-family: 宋体; margin-left: 10px; }
.dt_Title .inquiry strong.btn a { color: #ff9c00!important; text-decoration: underline; }
.dt_Title .inquiry span.oi { margin-right: 8px; }
.dt_Title .inquiry span.oi img { vertical-align: middle; }
.brandinfo { width: 265px; float: right; margin: 24px 40px 0 0; }
.brandinfo span { width: 100%; display: block; white-space: nowrap; line-height: 20px; }
.dt_2 { width: 100%; float: left; border-top: #e9e9e9 1px solid; }
.dt_2Left { width: 720px; float: left; padding-top: 10px; }
.dt_2Left .info-con { width: 270px; float: right; }
.dt_2Left .price { width: 100%; float: left; padding: 5px 0px; font-weight: bold; color: #666666; }
.dt_2Left .price strong { font-size: 20px; font-family: 微软雅黑; color: #e10000; }
.dt_2Left .btn { width: 100%; float: left; margin-bottom: 15px; }
.dt_2Left .btn a { width: 110px; height: 28px; line-height: 28px; text-align: center; background: #e4e4e4; color: #666666; font-weight: bold; float: left; margin-right: 10px; }
.dt_2Left .btn a.inquery { background-color: #ff9c00; color: #fff!important; }
.dt_2Left ul.list { width: 100%; float: left; }
.dt_2Left ul.list li { width: 100%; height: auto; line-height: 22px; padding: 3px 0px; float: left; border-bottom: #e9e9e9 1px dotted; }
.dt_2Left ul.list li dt { width: 70px; height: 22px; float: left; padding-left: 25px; background: url(/images/agent_point_01.gif) 5px -1px no-repeat; }
.dt_2Left ul.list li dd { width: 165px; float: right; padding-right: 10px; }
.rollBox { width: 424px; height: 72px; float: left; overflow: hidden; position: relative; margin-top:8px;}
.rollBox .hd { width: 100%; height: 0px; position: absolute; left: 0px; top: 0px; }
.rollBox .hd a { display: block; width: 10px; height: 45px; position: absolute; top: 16px; background: url(/images/ed_rightbtn.gif) no-repeat; cursor: pointer; overflow: hidden; }
.rollBox .hd a.prev { left: 0px; background-position: 0px 0px; }
.rollBox .hd a.next { right: 0px; background-position: -10px 0px; }
.rollBox .bd { width: 380px; height: 70px; margin-left: 20px; overflow: hidden; }
.rollBox .bd ul { width: 100%; float: left; }
.rollBox .bd ul li { width: 95px; height: 70px; float: left; text-align: center; line-height: 0px; font-size: 0px; }
.dt_2Left .name { text-align: center; font: 700 14px/28px 微软雅黑; color: #666666; }
.detailContain { clear: both; padding-top: 10px; }
.detail-con { width: 746px; float: left; padding-top:15px;}
.detail-content { border: #e9e9e9 1px solid; border-top: none; background: #f7f7f7; padding: 10px; clear: both; }
.pdPart { clear: both; padding-bottom: 15px; color: #666666; }
.parameter { border-bottom: #ffffff 1px solid; clear: both; }
.parameter ul { width: 100%; float: left; }
.parameter ul li { width: 240px; float: left; border-top: #fff 1px solid; border-right: #fff 1px solid; background-color: #ffffff; height: 28px; }
.parameter ul li dt, .parameter ul li dd { line-height: 20px; padding: 5px; }
.parameter ul li dt { width:88px; float: left; border-right: #fff 1px solid; text-align: right; padding: 5px 0px; }
.parameter ul li dd { width:140px; float: right; }
.parameter ul li.gbg { background-color: #f2f2f2; }
.pdPart .title { width: 720px; height: 33px; line-height: 32px; border: #e6e6e6 1px solid; margin-bottom: 5px; }
.pdPart .title h3 { font-size: 12px; font-weight: bold; padding-left: 30px; background: url(/images/pd_point.gif) 6px 11px no-repeat; }
.pdPart .content { width: 705px; margin: 10px; }
.pdPart .content img { max-width:100%;}

.parHd { width: 744px; height: 29px; border: #dddddd 1px solid; border-bottom: #ff9c00 2px solid; float: left; background-color: #f1f1f1; margin-bottom: 0px; position: relative; }
.parHd ul { width: 100%; height: 29px; }
.parHd ul li { width: 114px; height: 29px; float: left; border-right: #dddddd 1px solid; }
.parHd ul li h2 { font: 700 12px/28px Arial; width: 114px; text-align: center; height: 29px; cursor: pointer; }
.parHd ul li.on { position: relative; }
.parHd ul li.on h2 { width: 116px; height: 32px; background-color: #ff9c00; color: #fff; position: absolute; left: -1px; top: -1px; }
.parHd span.more { position: absolute; right: 10px; top: 0px; height: 28px; line-height: 28px; font-family: 宋体; }
.blueTab .parHd { border-bottom-color: #3963c5; }
.blueTab .parHd ul li.on h2 { background-color: #3963c5; }
.blueTab .bigTitle { width: 978px; }





