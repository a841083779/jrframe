    </div>
  </div>
  <div class="bm">
    <a style="display:none" href="javascript:void(0);" onclick="addShop();return false;"><img src="/Static/img/bm.png">收藏</a>
  </div>
</div>
<div class="proT">
  <ul class="idTabs">
    <li><a href="#pt01" class="selected">产品性能</a></li>
    <li><a href="#pt02">产品参数</a></li>
    <li><a href="#pt03">产品评论</a></li>
  </ul>
</div>
<div class="proC">
  <div id="pt01" class="pcc">
  <#if (model.introduce?? && model.introduce?length > 11)>${model.introduce?replace("?","")}<#else>暂无介绍</#if>
  </div>
  <div id="pt02" class="pcc">
  <#if (model.paras?? && model.paras?length > 15)>${model.paras?replace("m3","m<sup>3</sup>")?replace("m2","m<sup>2</sup>")?replace("?","")}<#else>暂无参数</#if>
