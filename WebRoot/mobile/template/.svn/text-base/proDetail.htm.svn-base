<div class="swiper-container index" style="height:200px;overflow:hidden">
  <a href="javascript:void(0)" class="arr_ arr_left"></a>
  <a href="javascript:void(0)" class="arr_ arr_right"></a>
  <div class="swiper-wrapper">

<#if model.img2??&&(model.img2!='')> 
 <div class="swiper-slide"><a href="javascript:void(0)"><img title="${model.factoryname}${model.name}${model.catalogname}" src="/uploadfiles/${model.img2?replace('image', '')}"  onerror="showImgDelay(this,'/uploadfiles/no_small.gif',2);" ></a></div>
</#if>

  <#list query("select name,img_name from pro_product_pic where product_id =${model.id?c} ") as pics>
     <#if pics.img_name?contains("uploadfiles")>
          <div class="swiper-slide"><a href="javascript:void(0)"><img title="${pics.name}" src="http://m.product.21-sun.com/${pics.img_name?replace('image', '')}"  onerror="showImgDelay(this,'/uploadfiles/no_small.gif',2);" ></a></div>
      <#elseif pics.img_name?contains("image")>
          <div class="swiper-slide"><a href="javascript:void(0)"><img title="${pics.name}" src="http://m.product.21-sun.com/uploadfiles/${pics.img_name?replace('image', '')}"  onerror="showImgDelay(this,'/uploadfiles/no_small.gif',2);" ></a></div>
	 <#else>
          <div class="swiper-slide"><a href="javascript:void(0)"><img title="${pics.name}" src="http://m.product.21-sun.com/uploadfiles/${pics.img_name?replace('image', '')}"  onerror="showImgDelay(this,'/uploadfiles/no_small.gif',2);" ></a></div>
	 </#if>
  </#list>	
  </div>
  <div class="pagination"></div>
</div>
<div class="detailT">
  <div class="dtt">
    <h1 class="proName">${model.factoryname}${model.name}${model.catalogname}</h1>
    <div class="p">参考价格：<font>
	    <#if (model.price_start??&&model.price_start!=0)&&(model.price_end??&&model.price_end!=0)>
