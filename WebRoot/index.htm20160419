<script type="text/javascript"> 
jQuery(".mid").idTabs("!mouseover");
	//下方广告
	jQuery.each(jQuery("div[id^=ad_]"),function(){
		var id = jQuery(this).attr("id");
		jQuery("#"+id).load("/ad/"+id+".htm");	
	});
	
	function submitTuan(){
		var name = $.trim($("#txt_name").val());
		if(name == ""){
			alert("请输入姓名");
			$("#txt_name").focus();
			return;
		}
		var phone = $.trim($("#txt_phone").val());
		if(phone == ""){
			alert("请输入电话");
			$("#txt_phone").focus();
			return;
		}
		var reg = /^(0\d{2,3}\s?[\-]?\s?\d{7,8})|(1\d{10})$/;
		if(!reg.test(phone)){
			alert("请输入正确的电话，座机需要填写区号");
			$("#txt_phone").focus();
			return;
		}
		var company = $.trim($("#txt_company").val());
		if(company == ""){
			alert("请输入公司名称");
			$("#txt_company").focus();
			return;
		}
		
		$.ajax({
			type:"post",
			url:"/action/ajax.jsp",
			data:{flag:"tuan_form", username:name, telphone:phone, compname:company},
			success:function(result){
				$(".tuan-con").find(".t_input").val("");
				alert("报名成功，我们会尽快与您取得联系");
			}
		});
	}
	
	function showAuto3(){}
</script>
