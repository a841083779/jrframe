@charset "utf-8";
/* CSS Document */
.act_sem{ font-family:微软雅黑; background:#f3f3f3;}
.act_sem .topbg{ display:none;}
.sem_banner{ height:481px; background:url(../images/st_banner.jpg) no-repeat top center;}
.sem_val_1{ height:324px; background:url(../images/guize.jpg) no-repeat 0 0; margin-top:15px;}

.sem_val_2{ background:#fff; margin-top:30px;}
.sem_prolist{ width:930px; padding:25px; overflow:hidden;}
.sprolist{ width:110%;}
.sprolist li{ height:221px; overflow:hidden;}
.sprolist li li { height:28px;}
.sprolist li{ width:450px; float:left; margin:0 20px 20px 0;}
.sprolist li .sem_img{width:240px; float:left;}
.sprolist li img{ float:left; width:100%; height:200px; margin-right:10px;}
.sprolist li .sem_right{ float:right; width:200px;}
.sprolist li .sem_pname{ font-size:24px; font-weight:bold; color:#000; margin-bottom:10px;}
.sprolist li .sem_para{border-top: #c2c2c2 1px solid;border-bottom: #c2c2c2 1px solid;}
.sprolist li .sem_para li{ float:none; width:100%; line-height: 28px;font-size: 12px;border-bottom: #c2c2c2 1px dotted; text-indent:10px; margin:0;}
.sprolist li .sem_para .evenbg {background: #e8e8e8;}
.sprolist li .sem_btn{ display:inline-block; width:130px; height:40px; line-height:40px; font-size:16px; text-align:center; background:#ff6201; color:#fff; margin-top:15px;}

.sem_val_3{ height:95px; background:url(../images/lant.gif) no-repeat center; margin-top:30px;}

.sem_val_4{ background:#fff; padding:20px 0 30px; margin-bottom:30px;}
.p4_con{ height:auto; background-color:#fcfffd; clear:both; padding-top:10px;}
.p4_con table b{ padding-right:10px; color:#c93316}
.p4_con table select{ width:220px; margin-right:10px; color:#666666; height:22px; line-height:22px; border:1px solid #ccd3e4}
.p4_con table textarea{ width:640px; height:100px;}
.xing{ line-height:30px; padding-top:3px;}
.xing span{padding-left: 15px;vertical-align: 3px;}
span.tip{ padding:10px 100px 10px 10px; background-color:#edf5fa; color:#0099fd}
.ti_btn{ display:block; background:url(../images/ti2_btn.png); width:273px; height:53px; font:0px a; letter-spacing:-7px; margin-left:180px; margin-top:5px; margin-bottom:12px; border:0;}
.xing,.xing_tip{ display:inline-block; line-height:33px;*display:inline;*zoom:1}
.xing_tip{ padding-left:5px; vertical-align:3px;}

.act_sem .proFoot .New_website{ display:none;}
.act_sem .proFoot .fewm{ top:0px !important;}
.act_sem .proFoot .New_foot{ padding-top:20px;}


.sem_val_new{ padding:25px; width:930px; background-color:#fff; margin-top:30px;}
.sem_val_new li{ width:100%; float:left; height:34px; line-height:34px;}
.sem_val_new li.ll1{ border-bottom:#ededed solid 1px;}
.sem_val_new li .prize{ width:305px; float:left;}
.sem_val_new li{ font-size:14px;}
.sem_val_new h3{ color:#c74b33; font-size:24px;}
.sem_val_new li span{ display:inline-block; width:100px;}
.sem_val_new li b{ display:inline-block; width:70px; font-weight:normal;}
.sem_val_new i{ color:#c74b33; font-style:normal;}
.sp{ width:160px !important;}