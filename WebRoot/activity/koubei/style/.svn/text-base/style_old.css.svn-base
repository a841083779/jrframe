.loginRight { width:276px; height:460px; float:right; border-left:#cccccc 1px dotted; padding-left:68px;}
input.b_lb,a.b_lb { display:block; width:121px; height:35px; background:url(../images/b_login.png) 0px -72px no-repeat; text-align:center; color:#fff!important; font:100 18px/38px 微软雅黑;}
input.b_lb,a.b_lb1 { cursor:pointer; display:block; width:153px; height:40px; background:url(../images/btn_07.png) 0px 0px no-repeat; text-align:center; color:#fff!important; font:0px a; letter-spacing:-7px;}
a.b_lb2 { cursor:pointer; display:block; width:153px; height:40px; background:url(../images/btn_03.png) 0px 0px no-repeat; text-align:center; color:#fff!important; font:0px a; letter-spacing:-7px;}
.imgError{display: none;width: 18px;height: 18px;float: left;margin-top: 10px;margin-left: 6px;}
.mb20{ margin-bottom:20px;}
.mt15{ margin-top:15px;}