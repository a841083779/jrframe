/* CSS Document */
.topbg{ display:none}
.topbg{ margin-bottom:0px !important;}
.contain960 { width:960px; margin:0 auto; clear:both; font-family:微软雅黑}

.wrap_out{ background:url(../images/bbg.png) repeat-x;}
.ptbg{ background:url(../images/p1tbg.png) top repeat-x;}
.part1{ height:443px; background:url(../images/1_03.jpg) no-repeat; overflow:hidden}
.part1 strong{ display:block; font-size:36px; color:#fff; font-weight:bold; margin-top:90px; margin-left:210px;}
.part1 strong span{ font-size:20px; font-weight: normal}
.part1 a{ display:block; width:178px; height:50px; background:url(../images/share_btn.png) no-repeat; font:0px a;margin-top: 250px;margin-left: 400px;}
.part2{ height:177px; background:url(../images/1_07.png) no-repeat;}
.part2 p{ width:745px; margin-left:190px; color:#fff; font-size:13px; padding-top:10px;}

.part3{ margin-top:15px; background: url(../images/p3_bg.png) no-repeat; width:963px;}
.part3_con1{ width:380px;}
.part3_con1 strong,.part3_con1 span{ display:block}
.part3_con1 strong{ height:88px;font-size:32px; font-weight:bold; color:#1d5ebc; text-align:left; padding-left:15px; padding-top:15px;}
.part3_con1 span{ height:147px;padding:12px 15px 0; color:#5e5e5e; font-size:14px; font-weight:normal; line-height:29px;}
.part3_con1 span b{ color:#1d5ebc; font-size:19px;}

.part3_con2{ width:573px;}
.p3_rule{ padding-left:51px;height:123px; width:347px; margin-top:30px;}
.p3_rule p{ padding:10px; line-height:22px;}
.p3_rule p b{ color:#ff6c00; font-size:14px;}
.p3_pre{padding-left:51px;height:108px; width:555px; margin-top:4px;}
/*.big_img{ position:absolute;top:-50%;display:none;}*/
.big_img{ display:none; position:absolute; top:-50%; left:-60%; z-index:30;}
.img_2{ left:-80%}
.p3_pre ul li{ float:left; margin-left:15px; padding-top:5px;_display:inline;}
.p3_pre ul li img{ cursor:pointer;}
.p3_pre ul li.t{ margin-top:-150px; margin-left:10px;}
.p3_pre ul li span{ display:block; font-weight:bold; margin-top:5px;}

.part4{ color:#666666; margin-top:20px;}
.part4 .title{ height:auto;}
.part4 .title img{ float:left}
.part4 .title span{ float:right; line-height:12px; padding-top:55px;}
.p4_con{ height:auto; background-color:#fcfffd; clear:both; padding-top:10px;}
.p4_con table{ }
.p4_con table b{ padding-right:10px; color:#c93316}
.p4_con table select{ width:220px; margin-right:10px; color:#666666; height:22px; line-height:22px; border:1px solid #ccd3e4}
.p4_con table textarea{ width:640px; height:100px;}
.xing{ line-height:30px; padding-top:3px;}
.xing span{padding-left: 15px;vertical-align: 3px;}
span.tip{ padding:10px 100px 10px 10px; background-color:#edf5fa; color:#0099fd}
.ti_btn{ display:block; background:url(../images/ti_btn.png); width:270px; height:52px; font:0px a; letter-spacing:-7px; margin-left:180px; margin-top:5px; margin-bottom:12px; border:0;}
.part5{ background-color:#fcfffd; color:#999; margin-top:20px;}
.listhead b{ display:block; width:25%;  text-align:center; float:left; height:40px; line-height:40px;}
.listbody { width:100%; height:390px;*height:420px; overflow:hidden;}
.listbody ul li{ overflow:hidden; background:url(../images/btbg_11.png) bottom center no-repeat;}
.listbody ul li strong,.listbody ul li span{ display:block; width:25%; float:left; height:65px; line-height:65px; text-align:center; font-weight:normal;}
.listbody ul li strong img{ vertical-align:middle; margin-right:15px;}
.listbody ul li span{ color:#ea4609}
.rbtip{ text-align:right; height:55px; line-height:55px;  padding-right:30px;}
.rbtip b{ font-weight:bold; color:#ea4609}
.part6 ul{ width:995px;*padding-bottom:20px;}
.part6 ul li{ float:left; width:20%; text-align:left; margin-bottom:20px;}
.part6 ul li strong{ display:block; font-weight:normal; color:#fff; background-color:#1d5ebc; width:164px; text-align:center; padding:3px 0;white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.p6_title{ margin:15px 0;}
.td_bot{ border-bottom:1px solid #e5faec}

.xing,.xing_tip{ display:inline-block; line-height:33px;*display:inline;*zoom:1}
.xing_tip{ padding-left:5px; vertical-align:3px;}

#addImgs_show li{float:left;height:70px;margin-right:10px;position:relative;}
.img_delete{display:none;}
#addImgs_show li.first{height:auto;}
#addImgs_show li.first img.img { width:160px;*height:120px; height:125px;border-style:solid;border-width:3px;border-color:#97b6d3;}
#addImgs_show li.first a.img_delete { left:155px!important;}


/*注册页面*/
.wrapper { width:1000px; margin:0 auto; clear:both; }
.loginLeft { width:625px; float:left;}
.registContent { padding-left:26px;}
.registContent li.rcl { width:100%; height:36px; float:left; padding-bottom:15px; margin:0; border:0;}
.registContent dl.rcd { width:369px; height:36px; float:left; background:#fff;}
.registContent dl.rcd dt { width:85px; float:left; white-space:nowrap; height:36px; line-height:36px; padding-left:8px; color:#333333;text-align: right;}
.registContent dl.rcd dd { border-radius: 4px;width:270px; border:#cccccc 1px solid; float:left; height:36px; padding-left:4px;}
.registContent dl.rcd dd input.bri { width:267px; height:26px; line-height:26px; padding:5px 0px; border:none; background:none; font-size:14px; font-family:微软雅黑; color:#222222;}
.registContent dl.rcd dd.radio { padding-top:12px; height:24px;}
.registContent dl.rcd dd.radio input { margin-left:20px;}
/*.registContent dl.rcd dd.select { padding-left:10px;}*/
.registContent dl.rcd dd.select select { width:218px; padding:4px; border:none; background:none; margin-top:8px;margin-left: 20px;}
.registContent dl.rcd dd.select_area select { width:70px; padding:4px; border:none; background:none; margin-top:5px;} 
.registContent div.rfRight span { line-height: 36px; color: #999; padding-left: 5px;}

.registContent dl.yanzhengma { width:235px;}
.registContent dl.yanzhengma dd input.bri { width:124px;}
.registContent dl.yanzhengmaImg { width:126px; float:left; padding-left:10px;}
.registContent .xieyi,.registContent .b_btn { width:100%; float:left; padding-bottom:15px;}
.registContent .xieyi input { vertical-align:-2px;}
.loginContainer { width:980px; background-color:#fff; border:#ccc 1px solid; padding:30px 0px 0px 0px; margin-top:30px;}
