html, body { overflow-x:visible;}
a:hover { color:#fe6000;}
.topbg { background-position:center 8px;}
.mb20 { margin-bottom:20px;}

.comNav { height:30px; padding-top:5px; background:url(../images/new_navbg.png) center top repeat-x; position:relative; z-index:300;}
.comNav .fix { padding:0px 0px;}
.comNav li { float:left; font-size:14px; font-family:"Microsoft Yahei"; margin-right:15px; position:relative;}
.comNav a { display:inline-block; width:auto; height:27px; line-height:26px; padding:0px 40px;}
.comNav .cur a,.comNav a:hover,.comNav .hover a { color:#fff; background-color:#fe6000;}
.comNav li .box { display:none; width:100%; height:auto; background-color:#fff; border:#ededed 1px solid; box-shadow:#ccc 0px 2px 2px; border-radius:0px 0px 5px 5px; position:absolute; left:-1px; top:27px;}
.comNav li .box a { display:block; width:100%; text-align:center; padding:0; background:none; color:#666!important;}
.comNav li .box a:hover { color:#fe6000!important;}
.comNav li .box i { display:block; width:0; height:0; overflow:hidden; border-color:transparent transparent #fff; border-style:dashed dashed solid; border-width:5px; position:absolute; left:50%; margin-left:-5px; top:-10px;}
.comNav li.hover .box { display:block;}

.kb_cates { width:210px; height:300px; float:left; background-color:#fff; padding:0px; font-family:"Microsoft Yahei"; position:relative; margin-right:10px; z-index:10;}
.kbc { border-bottom:#cccccc 1px dashed; padding:5px 10px;}
.kb-hd { font-size:14px;}
.kb-hd i { display:inline-block; width:30px; height:30px; vertical-align:middle; background:url(../images/kb_cates.png) no-repeat; overflow:hidden; margin-right:3px;}
.kb-hd .kbc1 { background-position:0 0;}
.kbc-hover .kbc1 { background-position:-30px 0;}
.kb-hd .kbc2 { background-position:0 -30px;}
.kbc-hover .kbc2 { background-position:-30px -30px;}
.kb-hd .kbc3 { background-position:0 -60px;}
.kbc-hover .kbc3 { background-position:-30px -60px;}
.kb-hd .kbc4 { background-position:0 -90px;}
.kbc-hover .kbc4 { background-position:-30px -90px;}
.kb-hd .kbc5 { background-position:0 -120px;}
.kbc-hover .kbc5 { background-position:-30px -120px;}
.kb-bd { clear:both; overflow:hidden; padding-top:2px;}
.kb-bd a { display:inline-block; width:auto; height:auto; white-space:nowrap; margin-right:10px;}
.kb-box { display:none; width:215px; height:278px; padding:10px; border:#e4e4e4 1px solid; box-shadow:#ccc 0px 0px 2px; background-color:#fff; position:absolute; left:210px; top:0px;}
.kbc-hover {/* position:relative;*/ background-color:#eeeeee;}
.kbc-hover .kb-box { display:block;}
.kb-box .t { display:block; border-bottom:#cccccc 1px dashed; height:30px; line-height:30px; padding-left:5px;}
.kb-box ul { overflow:hidden; clear:both; padding-top:5px; padding-left:5px;}
.kb-box li { width:33.3%; height:28px; line-height:28px; float:left;}

.kbSlider { width:520px; height:300px; float:left; overflow:hidden; position:relative;}
.kbSlider .bd { width:520px; height:300px; position:relative;}
.kbSlider .bd ul { width:100%; float:left;}
.kbSlider .bd li { width:520px; height:300px; float:left;}
.kbSlider .hd { width:100%; height:34px; position:absolute; left:0px; bottom:0px; background:rgba(0,0,0,.60); filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#99000000,endcolorstr=#99000000);}
.kbSlider .hd ul { width:102%;}
.kbSlider .hd li { width:33px; height:34px; line-height:34px; border-right:#000 1px solid; float:left; text-align:center;}
.kbSlider .hd li b { color:#fff; font-weight:normal;}
.kbSlider .hd li span { display:none;}
.kbSlider .hd .on { text-align:left;}
.kbSlider .hd .on b { padding:0px 12px;}
.kbSlider .hd .on span { display:inline; color:#fff;}

.sideRight { width:230px; float:right;}
.kb_mem { background-color:#fff; height:185px; padding:5px 0; margin-bottom:10px;}
.kb_mem th { font-weight:normal; text-align:right;}
.kb_mem th,.kb_mem td { line-height:32px; padding:5px;}
.kb_mem .input { width: 160px; height:30px; border:#ddd 1px solid;}
.kb_mem .loginBtn { display:inline-block; width:92px; height:30px; background:url(../images/login_btn.png) no-repeat; overflow:hidden; font:0/0 a; letter-spacing:-9px; vertical-align:middle; margin-right:5px;}

.gonggao { height:85px; background:#fff; padding:5px 10px;}
