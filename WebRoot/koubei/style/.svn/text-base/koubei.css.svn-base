.memInfo .info .focus { display:block; padding-left:20px; width:31px; height:22px; line-height:22px; background:url(../images/focus.png) no-repeat;}
.memInfo .links { clear:both; overflow:hidden; padding:15px 0 10px;}
.memInfo .links img { vertical-align:middle;}
.memInfo .links a { margin-right:15px;}
.memInfo .out { text-align:center;}
.memInfo .outBtn { display:inline-block; width:111px; height:30px; background:url(../images/out_btn.png) no-repeat; font:0/0 a; letter-spacing:-9px;}

