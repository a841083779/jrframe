<script>
$('#tlj_move a').mouseover(function(){
		$(this).parent().addClass('tab_t_cur').siblings().removeClass('tab_t_cur');
		$('#wjj_list .txd_mpro_list').css('display','none');
		$('#wjj_list .txd_mpro_list').eq($(this).parent('li').index()).css('display','block');
      });
