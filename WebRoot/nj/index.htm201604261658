<script>
	var jrChannel='0#0';
	seajs.use('index/index.js')
    define("page_use",function(require,exports,module) {
	    var $ = require('jquery');
	    var base = require('base');	
    });
    seajs.use("page_use");
	function tj(){ 
		  if(jQuery('#keyword').val()==''){
		     alert("请输入您要搜索的关键词");
		     jQuery('#keyword').focus();
			 return false;
		  }
		  var keyword=jQuery('#keyword').val();
		  window.open("http://product.21-sun.com/search/"+keyword+"_0_0_0.htm");
	}
	$('#keyword').on('keydown', function (e) { if (e.keyCode == 13) {tj()} })
	//退出
    function logout(){
	    jQuery.ajax({
		    url:'action/ajax.jsp?flag=logout&r='+new Date(),
		    async:false,
		    cache:false,
		    success:function(data){
			    alert("您已退出登录！");
			    setTimeout("window.location.reload()",1000);
		    }
	    });
    }
	if (window.jereiReceiver) { 
        var _jrTracker= jereiReceiver.getTracker("GWD-100002"); 
        _jrTracker.trackIde(/id=.?gg_\w+.?/ig); 
    }
</script>
