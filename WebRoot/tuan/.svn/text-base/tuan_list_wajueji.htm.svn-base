		n5 = n5 >=(count5 - 1) ? 0 : ++n5;
		jQuery("#tuan_banner ul.tuan_p li").eq(n5).trigger('click');
	}
}
function showPre5(){
	n5 = n5 <=0 ? (count5 - 1) : --n5;
	jQuery("#tuan_banner ul.tuan_p li").eq(n5).trigger('click');
}
function pase(){
	if(paseState==0){
		paseState=1;
	}else{
		paseState=0;
	}
}
</script>
