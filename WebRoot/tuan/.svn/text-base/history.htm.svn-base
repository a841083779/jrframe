<script type="text/javascript">
var t5,n5 = 0, count5, paseState=0;
jQuery(document).ready(function(){	
	count5=jQuery("#tuan_banner ul.tuan_i li").length;
	jQuery("#tuan_banner ul.tuan_i li:not(:first-child)").hide();
	jQuery("#tuan_banner ul.tuan_p li").click(function() {
		var i = jQuery(this).attr("value") - 1;
		n5 = i;
		if (i >= count5) return;
		jQuery("#tuan_banner ul.tuan_i li").filter(":visible").fadeOut(500).parent().children().eq(i).fadeIn(800);
		jQuery(this).toggleClass("selected");
		jQuery(this).siblings().removeAttr("class");
	});
	t5 = setInterval("showAuto5()", 4000);
	jQuery("#tuan_banner").hover(function(){clearInterval(t5)}, function(){t5 = setInterval("showAuto5()", 4000);});
})
function showAuto5(){
	if(paseState==0){
		n5 = n5 >=(count5 - 1) ? 0 : ++n5;
		jQuery("#tuan_banner ul.tuan_p li").eq(n5).trigger('click');
	}
}
function showPre5(){
	n5 = n5 <=0 ? (count5 - 1) : --n5;
	jQuery("#tuan_banner ul.tuan_p li").eq(n5).trigger('click');
}
function pase(){
	if(paseState==0){
		paseState=1;
	}else{
		paseState=0;
	}
}
</script>
