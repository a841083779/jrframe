.nav li.cur a{ color:#d51938; background:#fff; border-radius:3px;}
.footer{ height:157px; padding-top:25px; color:#fff; background:#14a4a5; text-align:center; position:relative;}
.footer .code{ position:absolute; right:115px; top:5px;}