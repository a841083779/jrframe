		jQuery("#tuan_banner ul.tuan_i li").filter(":visible").fadeOut(500).parent().children().eq(i).fadeIn(800);
		jQuery(this).toggleClass("selected");
		jQuery(this).siblings().removeAttr("class");
	});
	t5 = setInterval("showAuto5()", 4000);
	jQuery("#tuan_banner").hover(function(){clearInterval(t5)}, function(){t5 = setInterval("showAuto5()", 4000);});
})
function showAuto5(){
	n5 = n5 >=(count5 - 1) ? 0 : ++n5;
	jQuery("#tuan_banner .tuan_p li").eq(n5).trigger('click');
}
</script>
