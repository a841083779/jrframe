.con02 { width:928px; height:667px; position:absolute; left:50%; top:50%; margin:-334px 0px 0px -464px; top:340px; margin-top:0px; background:url(../images/con2bg.png) no-repeat;}
.con02 h2 { height:50px; line-height:50px; padding-left:30px; font-size:20px; color:#fff; margin-bottom:20px;}
