.con02 p.text .st01 { line-height:34px; font-size:24px; font-weight:normal;}
.con02 p.text font { font-size:24px; color:#004199; text-decoration:underline;}

.list03 { width:894px; height:auto; margin:0 auto 20px; overflow:hidden; clear:both; position:relative;}
.list03 h3 { height:50px; line-height:50px; font-size:20px; padding-left:13px; background:url(../images/t_p01.gif) left 50% no-repeat; color:#282828;}
.list03 ul { width:110%;}
.list03 li { width:119px; height:214px; float:left; margin-right:10px; position:relative;}
