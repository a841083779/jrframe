<script type="text/javascript">
$(document).ready(function(){
  $(".lottery_flash img").click(function(){
	   $(".lotteryLayer,.blackCover").show();
	  });	
  $(".lotteryLayer a.close").click(function(){
	   $(".lotteryLayer,.blackCover").hide();
	  });
  $(".loginTip a").click(function(){
	  $(".loginTip").hide();
	  $(".loginForm").show();
	  });
});
function setPriseType(){
	//$(".lotteryLayer_tip,.blackCover").show();
	if(true){
		alert("抽奖活动已经结束！");
		return -1;
	}
	var panDuan = panDuanChouJiang();
	if(panDuan=='no'){
		return 1;
	}
	if(panDuan=='ok'){
		return 0;
	}
	if(panDuan=='over'){
		alert("对不起，该手机号的抽奖机会已经用完！");
		return -1;
	}
	//显示登录层
	showLoginLayer();
	return -1;
}

function panDuanChouJiang(){
	var rs = -1;
	$.ajax({
		url : "action.jsp",
		async : false ,
		data : {
			flag : 'panduan'
		},
		type : "post",
		success : function(data){
			rs = $.trim(data);
		}
	});
	return rs;
}
function hideLoginLayer(){
	$(".blackCover").hide();
	$(".loginLayer").hide();
	$(".loginLayer .loginTip").show();
	$(".loginLayer .loginForm").hide();
}
function showLoginLayer(){
	$(".blackCover").show();
	$(".loginLayer").show();
	$(".loginLayer .loginTip").hide();
	$(".loginLayer .loginForm").show();
}


$("#theform").Validform({
	tiptype : 4,
	ajaxPost : true,
	callback : function(json){
		if(json.status=='y'){
			alert("验证成功，您现在可以进行抽奖了！");
			hideLoginLayer();
		}
	}
});

function doSub(){
	$("#theform").submit();
}
function showinquery(pid,factoryid){
	jQuery.jBox("iframe:/include/products/product_order.jsp?productid="+pid+"&factoryid="+factoryid, {
	title: "在线询价",
	width: 720,
	height: 435,
	top: '20%',
	iframeScrolling : 'no',
	buttons: { '关闭': false }
	});
	jQuery(".jbox-button-panel").attr("style","display:none;") ;
}
</script>