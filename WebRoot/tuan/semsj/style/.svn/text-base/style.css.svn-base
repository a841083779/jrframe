@charset "utf-8";
/*去除页面中所有标签的默认间距、补白*/
* { margin:0px; padding:0px;}
/*页面中的默认字体、字体大小、背景、文字颜色*/
body { font-size:12px; font-family:Arial; color:#1f1f1f; background:#ccc; cursor:default; _position:relative; _height:100%;}
* html,* html body{ background-image:url(about:blank);background-attachment:fixed}
/*页面右侧滚动条*/
html { overflow-y:scroll;}
/*清除页面所有图片的默认边框*/img{border:none;}/*去除ol、ul、li的默认样式*/ol,ul,li{list-style-type:none;}/*设置表单等项目的默认字体大小*/input,select,textarea{font-size:12px;}/*设置文字段落、表格单元格的默认行高*/p,th,td{line-height:22px;}/*字体*/.yahei{font-family:微软雅黑;}.songti{font-family:宋体;}/* 其它需要用到的字体大小 */h1,h2,h3,h4,h5,h6{font-weight:normal;}em,i,u{font-style:normal;}h1{font-size:14px;}h2{font-size:12px;}h3{font-size:12px;}.font10{font-size:10px;}.font11{font-size:11px;}.font12{font-size:12px;}.font13{font-size:13px;}.font14{font-size:14px;}.font15{font-size:15px;}.font16{font-size:16px;}/*去除页面中的链接点击后留下的虚线框*/body a{outline:none;blr:expression(this.onFocus=this.blur());}/* 整个网站需要的字体样式*/.strong{font-weight:bold;}.underline{text-decoration:underline;}/* li相关 */ul,li{list-style-type:none;}/*默认的文字链接样式*/a:link,a:visited{color:#272727;text-decoration:none;}a:hover,a:active{color:#006aaf;text-decoration:none;}/*其他文字链接样式*/.cffffff{color:#ffffff;}.cffffff a:link,.cffffff a:visited{color:#ffffff;}.cffffff a:hover,.cffffff a:active{color:#cccccc;}/* w系列控制宽度 */.wauto{width:auto}.w25{width:25%;}.w33{width:33.3%;}.w50{width:50%;}.w100{width:100%;}.w100px{width:100px;}.w180px{width:180px;}.w120px{width:120px;}.w200px{width:200px;}.w300px{width:300px;}.w310px{width:310px;}.w400px{width:400px;}.w760px{width:760px;}.w960px{width:960px;}.w980px{width:980px;}.w1000px{width:1000px;}.w1344px{width:1344px;}/* h系列控制高度 */.h100{height:100%;}.hauto{height:auto;}.h20px{height:20px;}.h30px{height:30px;}.h40px{height:40px;}.h50px{height:50px;}.h100px{height:100px;}.h150px{height:150px;}.lineheight100{line-height:100%;}.lineheight120{line-height:120%;}.lineheight150{line-height:150%;}.lineheight180{line-height:180%;}.lineheight200{line-height:200%;}.lineheight250{line-height:250%;}.lineheight15px{line-height:15px;}.lineheight20px{line-height:20px;}.lineheight24px{line-height:24px;}.lineheight30px{line-height:30px;}.lineheight32px{line-height:32px;}.lineheight40px{line-height:40px;}/* p系列控制padding */.p5{padding:5px;}.p10{padding:10px;}.p15{padding:15px;}.p20{padding:20px;}.p25{padding:25px;}.p30{padding:30px;}.pt5{padding-top:5px;}.pt10{padding-top:10px;}.pt15{padding-top:15px;}.pt20{padding-top:20px;}.pt25{padding-top:25px;}.pt30{padding-top:30px;}.pr5{padding-right:5px;}.pr10{padding-right:10px;}.pr15{padding-right:15px;}.pr20{padding-right:20px;}.pr25{padding-right:25px;}.pr30{padding-right:30px;}.pb5{padding-bottom:5px;}.pb10{padding-bottom:10px;}.pb15{padding-bottom:15px;}.pb20{padding-bottom:20px;}.pb25{padding-bottom:25px;}.pb30{padding-bottom:30px;}.pl5{padding-left:5px;}.pl10{padding-left:10px;}.pl15{padding-left:15px;}.pl20{padding-left:20px;}.pl25{padding-left:25px;}.pl30{padding-left:30px;}/* m系列控制padding */.m5{margin:5px!important;}.m10{margin:10px!important;}.m15{margin:15px!important;}.m20{margin:20px!important;}.m25{margin:25px!important;}.m30{margin:30px!important;}.mt5{margin-top:5px!important;}.mt10{margin-top:10px!important;}.mt15{margin-top:15px!important;}.mt20{margin-top:20px!important;}.mt25{margin-top:25px!important;}.mt30{margin-top:30px!important;}.mr5{margin-right:5px!important;}.mr10{margin-right:10px!important;}.mr15{margin-right:15px!important;}.mr20{margin-right:20px!important;}.mr25{margin-right:25px!important;}.mr30{margin-right:30px!important;}.mb5{margin-bottom:5px!important;}.mb10{margin-bottom:10px!important;}.mb15{margin-bottom:15px!important;}.mb20{margin-bottom:20px!important;}.mb25{margin-bottom:25px!important;}.mb30{margin-bottom:30px!important;}.ml5{margin-left:5px!important;}.ml10{margin-left:10px!important;}.ml15{margin-left:15px!important;}.ml20{margin-left:20px!important;}.ml25{margin-left:25px!important;}.ml30{margin-left:30px!important;}/* 其他常用元素 *//*文本对齐样式*/.center{margin:0px auto;}.tcenter{text-align:center;}.tleft{text-align:left;}.tright{text-align:right;}/*左右漂浮样式*/.l{float:left;}.r{float:right;}/*css清除浮动*/.c{clear:both}.clear{clear:both;height:0px;line-height:0px;font-size:0px;}/*clearfix:after*/.clearfix:after{content:".";display:block;height:0px;clear:both;visibility:hidden;}.clearfix{display:inline-table;display:block;}*html .clearfix{height:1%;}*+html .clearfix{height:1%;}.imgfix img{display:block;}/*添加鼠标悬浮显示手的形状*/.hand{cursor:pointer;}/*单行截字*/.textbreak{white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}/*最小高度*/.minheight500px{min-height:500px;height:auto!important;height:500px;overflow:visible;}/*隐藏元素*/.hide{display:none;}/*解决IE6双边距加倍错误*/.inline{_display:inline;}/*通用样式*/.title{width:100%;float:left;}.title h2,.title h3{width:auto;float:left;white-space:nowrap;}.title span.more{width:auto;float:right;font-family:宋体;}.list{width:100%;float:left;}.list li{width:100%;float:left;}.img{float:left;}.intro{float:right;}.content{width:100%;float:left;}

/*CSS Begin*/
.wrapper { width:1000px; padding:0px 0px; margin:0 auto;}
.top { background-color:#fff;}
.banner { line-height:0px; font-size:0px;}
.mainbg01 { background:url(../images/sem_bg01.jpg) center top no-repeat; height:375px; clear:both; overflow:hidden;}
.mainbg02 { background:url(../images/sem_bg02.jpg) center top no-repeat; height:874px; clear:both; overflow:hidden;}
.contain { width:690px; float:left; margin-left:155px; _display:inline; padding:35px 0px;}

.contain .intro { width:100%; font-family:微软雅黑; position:relative;}
.contain .intro p.text { height:120px; line-height:22px; font-size:15px; overflow:hidden; background:url(../images/sem_line.png) center bottom no-repeat; _background:none; _border-bottom:#c4cfdb 1px solid; padding-left:35px;}
.contain .intro p.text strong { font-size:17px; font-weight:normal; color:#0674c2;}

.model { position:absolute; right:-120px; bottom:-88px;}

.form { width:100%; height:auto; float:left; padding-top:20px; position:relative;}
.form h3 { font:100 18px/30px 微软雅黑; color:#013878; padding-left:30px;}
.form ul { padding-left:30px;}
.form ul li { width:100%; float:left; padding:7px 0px;}
.form ul li strong.ft { width:auto; height:20px; float:left; font:100 14px/20px 微软雅黑; color:#013878;}
.form ul li strong.ft font { color:red; padding:0px 3px;}
.form ul li p.fc { width:400px; float:left;}
.form ul li p.fc input.input { width:140px; height:18px; line-height:18px; border:#cccccc 1px solid; background:#fff url(../images/inputbg.gif) top repeat-x; padding:0px 5px;}
.form ul li p.fc select.select { padding:2px; border:#ccc 1px solid;}
.form .btn { position:absolute; right:0px; bottom:5px;}
.form .btn input { width:90px; height:30px; font-size:20px; font-family:微软雅黑; color:#fdd60b; background-color:#0674c2; cursor:pointer; border:none;}

.tabContain { width:100%; float:left;}
.tabTitle { width:100%; float:left; height:29px; background:url(../images/tab_line.gif) bottom repeat-x; overflow:hidden;}
.tabTitle ul { width:100%; float:left; padding-left:30px;}
.tabTitle ul li { width:98px; height:28px; font:100 18px/26px 微软雅黑; text-align:center; float:left; border-bottom:#7793b2 1px solid; margin-right:130px; cursor:pointer;}
.tabTitle ul li.active { height:27px; border:#7793b2 1px solid; border-bottom:#e1e0e0 1px solid;}

.tabContain { width:100%; float:left;}
.tabContent { width:100%; padding:20px 0px; overflow:hidden;}
.ProIntro { width:100%; float:left;}
.ProIntro ul.imgs { width:100%; float:left; margin-bottom:15px;}
.ProIntro ul.imgs li { width:50%; float:left; text-align:center;}
.ProIntro ul.imgs li h3 { display:inline-block; background-color:#f8d11b; *width:100px; height:22px; line-height:22px; padding:0px 50px;}

.introText { width:100%; float:left; font-family:微软雅黑;}
.introText h3 { font:100 14px/30px 微软雅黑; height:40px; color:#013878;}
.introText ul { width:100%; float:left; margin-bottom:15px;}
.introText ul li { line-height:24px; background:url(../images/point.gif) left 14px no-repeat; padding:5px 0px 5px 30px; font-size:14px;}

.detailBtn { width:100%; float:left;}
.detailBtn a { display:block; width:96px; height:21px; padding-left:19px; font:100 14px/21px 微软雅黑; color:#000000!important; background:url(../images/more_btn.gif) no-repeat; float:right;}

.activeDetail { width:100%; float:left;}
.activeDetail p.text { font:100 14px/22px 微软雅黑; color:#424242; padding:0px 25px; background:url(../images/sem_line.png) bottom no-repeat; _background:none; _border-bottom:#c4cfdb 1px solid; padding-bottom:15px; margin-bottom:15px;}
.activeDetail p.text strong { font-size:14px; color:#0674C2; font-weight:bold;}

.activeDetail .introText ul li { color:#013878; padding-top:2px; padding-bottom:2px; background-position:left 10px;}
.activeDetail .introText ul li.about { margin-top:15px; color:#424242;}

p.shuoming { width:100%; float:left; font:100 18px/26px 微软雅黑; color:#013878;}

.nameList { width:632px; height:163px; float:left; overflow:hidden; margin-left:30px; _display:inline;}
.nameList ul { width:650px; float:left;}
.nameList li { width:185px; height:40px; float:left; border-right:#a1c2da 1px solid; border-bottom:#a1c2da 1px solid; font:100 14px/40px 微软雅黑; color:#013878; padding-left:25px;}

#success { display:none;}
.successCover { width:100%; height:100%; position:fixed; left:0px; top:0px; background-color:#000; filter:alpha(opacity=50); opacity:0.5; z-index:1000;}
* html .successCover {position:absolute; left:expression(eval(document.documentElement.scrollLeft)); top:expression(eval(document.documentElement.scrollTop));}
.successLayer { width:440px; height:240px; background-color:#fff; position:fixed; left:50%; top:50%; margin:-120px 0px 0px -220px; z-index:1001;}
* html .successLayer { /* ie6 hack */ position: absolute; margin-top: expression(0 - parseInt(this.offsetHeight / 2) + (TBWindowMargin = document.documentElement && document.documentElement.scrollTop || document.body.scrollTop) + 'px'); top:250px;}

img.closeBtn { width:33px; height:32px; position:absolute; right:0px; top:0px; cursor:pointer;}
.successLayer p.text { text-align:center; padding-top:48px; font-family:微软雅黑; color:#0674c2;}
.successLayer p.text strong { display:block; font:100 22px/24px 微软雅黑; color:#0674c2; height:40px;}
.successLayer p.text a.confirm { display:block; width:102px; height:23px; line-height:22px; text-align:center; background-color:#0674c2; color:#fff; margin:20px auto 0px;}







