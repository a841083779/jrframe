function addOrder(a,b){
	var total=$('#total').html();
    if(parseInt(total)<5){
	$('#selectOrder').append("<li class='chosed' id='order"+a+"'><img  width='78' height='37'  src='"+b+"'/><i onclick='removeOrder("+a+")'></i></li>");
    $('#total').html(parseInt(total)+1);
	$('#orderIds').val($('#orderIds').val()+a+",");
	}else{
	  alert("一次最多只能选择5款产品！");
	}
	
	document.getElementById('show2').style.display = "none";
	document.getElementById('show1').style.display = "block";
}
function removeOrder(a){
    $('#order' + a).remove();
    var total=$('#total').html();
	$('#total').html(parseInt(total)-1);
}
function submit(){
    var total=$('#total').html();
    if(parseInt(total)==0){
	  alert("请先选择需要预定的产品!");	
	}else if(($('#contact').val()=='请输入手机号码或邮箱')||($('#contact').val()=='')){
	  alert("请填写您的联系方式！");
	}
	else{
	  $('#orderContact').val($('#contact').val());
	 
	  jQuery("#theform").ajaxSubmit({
		type : "POST",
		async : false ,
		success : function(data){
			alert("预定提交成功！");
			self.location.reload();
		   }					  
	  });
	 
