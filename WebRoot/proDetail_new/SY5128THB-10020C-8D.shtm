<script>
//询价城市选择
	$.fn.tab=function(){
		var $key=$(this).find('.xj_city_key'),
			 $val=$(this).find('.xj_city_val');
		$key.first().addClass('cur');
		$val.first().show();
		$key.each(function(index){
			$(this).click(function(event){
				event.preventDefault();
				$(this).addClass('cur').siblings().removeClass('cur');
				$val.hide().eq(index).show();
				})
			})
		};
	$('.xj_citybox_tab').each(function() { $(this).tab()});
	
	$('.xj_cityboxbtn').click(function(e) {
		$(this).addClass('xj_cityboxbtn_on');
		$('.xj_citybox').show();
	});
	
	$('.xj_city_val_01 .xj_cityshow span').click(function(e) {
		var pro_id=jQuery("#xj_zd_product_id").val();
		var prov = $(this).data('province');
		var prov_noe=prov.split('***');
		var area_id=prov_noe[1];
		$('#jr_province').val(prov_noe[0]);/*省份*/
		$('.xj_city_key_01').html(prov_noe[0]);
		$('.xj_city_key_01').removeClass('cur');
		$('.xj_city_key_02').addClass('cur');
		$('.xj_city_val_01').hide();
		$('.xj_city_val_02').show();
		jQuery.ajax({
					url : '/tools/ajax_city.jsp',
					async : false,
					type : 'post',
					data : {
						flag : 'get_city_new',
						p : area_id
					},
					success : function(data){
						$("#xj_zd_citys").html($.trim(data));	
						$('#xj_zd_citys span').click(function(e) {
								var city = $(this).data('city');
								$('#jr_city').val(city);/*城市*/
								$('.xj_city_key_02').html(city);
								var prov = $('.xj_city_key_01').html();
								$('.xj_city_key_02').removeClass('cur');
								$('.xj_city_key_01').addClass('cur');
								$('.xj_city_val_02').hide();
								$('.xj_city_val_01').show();
								$('.xj_cityboxbtn .tadd_p').html(prov);
								$('.xj_cityboxbtn .tadd_c').html(city);
								$('.xj_citybox').hide();
								$('.xj_cityboxbtn').removeClass('xj_cityboxbtn_on');
						});
					}
				});
	});
	
	$('.xj_city_close').click(function(e) {
		$('.xj_citybox').hide();
		$('.xj_cityboxbtn').removeClass('xj_cityboxbtn_on');
	});
	

</script>
