/* CSS Document */
.topbg { display:none;}
body { font-family:"Microsoft Yahei"; overflow:visible;}
a:hover { color:#fe6000;}

.s_brand_logo { width:auto; height:80px; float:left; white-space:nowrap;}
.s_brand_logo .s_img { float:left;margin-top:20px;}
.s_brand_logo .s_n { width:auto; height:28px; line-height:28px; border-left:#ddd 1px solid; font-size:24px; float:left; padding-left:12px; margin-top:30px; color:#636363;}
.s_brand_logo .s_n  h1{ font-size:24px;}
.s_mainSite { width:83px; height:23px; line-height:22px; color:#fff; font-weight:bold; float:right; background:url(../images/btnbg.png) no-repeat; text-align:center; margin-top:35px;}
.s_mainSite a { color:#fff!important;}
.s_tel { width:auto; float:right; font-size:14px; color:#333; height:23px; line-height:22px; margin-top:35px; padding-right:12px;}
.s_tel b { font-size:22px; font-style:italic; color:#ee0000;}

.s_navBar { clear:both; height:43px; background-color:#fe6000;}
.s_navBar li { width:auto; height:39px; float:left; border-right:#fd701a 1px solid; border-left:#eb5900 1px solid; padding:4px 2px 0;}
.s_navBar a { display:inline-block; width:auto; height:39px; line-height:38px; white-space:nowrap; padding:0px 14px; font-size:16px; color:#fff;}
.s_navBar .selected a,.s_navBar a:hover { background-color:#fff; color:#434343;}

.s_banner { font:0/0 a;}

.s_video { width:460px; height:275px; float:left; margin-right:15px;}
.s_intro .hd { padding-bottom:8px;}
.s_intro .hd h3 { font-size:24px; font-weight:bold; color:#434343; line-height:1.5;}
.s_intro .text { line-height:1.8; font-size:14px; color:#666;}

.s_mleft { width:750px; float:left;}
.s_right { width:210px; float:right;}

.comBox { clear:both; overflow:hidden;}
.comBox .hd { clear:both; height:40px; line-height:40px; border-bottom:#e2e2e2 5px solid; position:relative; margin-bottom:15px;}
.comBox .hd h3 { width:auto; height:40px; line-height:40px; position:absolute; left:0; top:0; font-size:24px; border-bottom:#fe6000 5px solid; padding:0px 10px 0px 5px;}
.comBox .hd .more { width:auto; float:right; font-family:"宋体";}
.comBox .bd { clear:both; overflow:hidden;}

.s_topNews { overflow:hidden; clear:both; position:relative; border-bottom:#eaeaea 1px solid; padding-bottom:15px; margin-bottom:10px;}
.s_topNews .img { width:175px; height:115px; float:left; margin-right:15px;}
.s_topNews .intro h3 { line-height:26px; padding:5px 0; font-size:18px; color:#565656; width:560px;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
.s_topNews .intro .text { line-height:22px; height:44px; overflow:hidden; margin-bottom:10px; color:#666;}
.s_topNews .intro .time { color:#999;}
.s_topNews .intro .btn { display:inline-block; padding:0px 15px; width:auto; height:22px; line-height:20px; background:url(../images/btnbg01.png) bottom repeat-x; position:absolute; right:0px; bottom:15px; border:#dddddd 1px solid;}
.s_news_list { clear:both; overflow:hidden; width:100%; border-bottom:#eaeaea 1px solid; padding-bottom:10px;}
.s_news_list ul { width:105%;}
.s_news_list li { width:44%; height:28px; line-height:28px; float:left; border-right:#e3e3e3 1px solid; background:url(/images/point01.gif) 3px 50% no-repeat; padding-right:3%; margin-right:2%;}
.s_news_list li a { width:270px; height:28px; line-height:28px; padding-left:10px; font-size:14px; white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
.s_news_list li .time { width:auto; float:right;}

.s_renwu { overflow:hidden; margin-top:-15px;}
.s_renwu ul { width:102%;}
.s_renwu li { width:210px; height:190px; float:left; border-right:#e9e9e9 1px solid; border-bottom:#e9e9e9 1px solid; padding:0px 20px; position:relative; padding-top:20px;}
.s_renwu img { display:block; width:210px; height:130px;}
.s_renwu .n,.s_renwu .p { display:block; line-height:20px;}
.s_renwu .n { font-size:14px;}
.s_renwu .focus { width:57px; height:18px; line-height:18px; background:#f8e3ae url(../images/add.png) 5px 50% no-repeat; border:#f6dc9a 1px solid; color:#88650c; display:block; position:absolute; right:20px; bottom:21px; text-indent:16px; overflow:hidden;}
.s_renwu .focused { background:#f4f4f4; border-color:#e9e9e9; text-indent:0; text-align:center; color:#999; cursor:default;}

.s_kb_total { background-color:#f7f7f7; clear:both; overflow:hidden; padding:15px; position:relative; margin-bottom:20px;}
.s_kb_total .s_total { width:auto; height:30px; line-height:30px; position:absolute; right:15px; top:50%; margin-top:-15px; font-size:14px;}
.s_kb_total .s_total b { color:#e60000;}
.s_kb_total .s_rank,.s_kb_total .s_point { display:block; line-height:1.8; font-size:14px;}
.s_kb_total .s_rank { font-size:18px; font-weight:bold;}
.s_kb_total .s_rank strong { font-size:30px; color:#e60000; margin-left:10px;}

.s_kb { clear:both; overflow:hidden;}
.s_kb .s_hd { border-bottom:#e9e9e9 3px solid; line-height:36px;}
.s_kb .s_hd h3 { width:auto; float:left; font-size:16px; color:#333; font-weight:normal; margin-right:20px;}
.s_kb .s_cate { width:520px; float:left; padding-top:8px;}
.s_kb .s_cate li { width:auto; height:18px; line-height:18px; float:left; border-right:#e0dae0 1px solid;}
.s_kb .s_cate li a { line-height:18px; display:inline-block; padding:0px 12px; font-size:16px;}
.s_kb .s_cate li a.selected { font-weight:bold;}
.s_kb .s_hd .more { width:auto; float:right; font-family:宋体;}

.s_bd { clear:both; overflow:hidden;}
.s_kb_list li { padding:15px 0; clear:both; overflow:hidden; position:relative; border-bottom:#e9e9e9 1px solid;}
.s_kb_list .user { width:60px; float:left; text-align:center; margin-right:15px;}
.s_kb_list .user img { display:block; width:60px; height:60px;}
.s_kb_list .intro { margin-left:75px;}
.s_kb_list .intro .n { font-size:16px; font-weight:bold; width:480px; height:20px; line-height:20px; display:block;white-space:nowrap;text-overflow:ellipsis;overflow:hidden; margin-bottom:5px;}
.s_kb_list .intro .time { width:auto; height:20px; line-height:20px; position:absolute; right:0px; top:15px; color:#999;}
.s_kb_list .intro .text { line-height:20px; height:40px; overflow:hidden; color:#666;}
.s_kb_list .intro .more { display:block; text-align:right; font-family:宋体;}

.s_more { display:block; text-align:center; clear:both;}
.s_more a { display:inline-block; width:auto; height:29px; line-height:28px; background:#f7f7f7 url(../images/more_btn.png) right no-repeat; padding:0px 36px 0px 20px; border:#e9e9e9 1px solid; border-top:none;}

.s_company { width:100%; overflow:hidden; border-bottom:#e9e9e9 1px solid;}
.s_company ul { width:100%;}
.s_company li { width:46%; height:205px; float:left; margin-right:4%;}
.s_company .n { display:block; font-size:18px; height:26px; line-height:26px; width:100%; font-weight:bold; white-space:nowrap;text-overflow:ellipsis;overflow:hidden; margin-bottom:3px;}
.s_company .area { display:block; color:#999;}
.s_company .text { line-height:20px; height:80px; overflow:hidden; margin-bottom:10px; padding-top:8px;}
.s_company .btn { display:inline-block; width:auto; height:28px; line-height:28px; background:url(../images/link_bg.png) repeat-x; padding:0px 20px; border:#e2e2e2 1px solid; font-size:14px;}

.comBox01 { border:#d3d3d3 1px solid; clear:both;}
.comBox01 .hd { background-color:#fe6000; height:30px; line-height:30px;}
.comBox01 .hd h3 { width:auto; height:30px; float:left; color:#fff; padding-left:10px;}
.comBox01 .hd .more { width:auto; float:right; font-family:"宋体"; padding-right:5px;}
.comBox01 .hd .more a { color:#fff;}
.comBox01 .bd { clear:both;}
.s_info { margin:10px; border-bottom:#eaeaea 1px solid; line-height:20px; padding:5px 0 10px;}
.s_weibo { width:185px;  padding-bottom:10px;padding-left:35px;}

.s_pro_cates li { border-bottom:#ccc 1px dotted; padding-left:20px; background:url(/images/point01.gif) 8px 50% no-repeat;}
.s_pro_cates a { display:block; width:100%; height:28px; line-height:28px; white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
.side_gg { border:0;}
.side_gg img { display:block; margin-bottom:10px;}

/*kc*/
.new_news .intro h3{ line-height:22px; padding:0; margin-bottom:10px;}
.intro .tag{line-height:25px; vertical-align:bottom; color:#204c89; background:#fff url(../images/n_03.png) bottom repeat-x; padding:0 10px 0 5px;}
.s_topNews .intro .text{ margin-bottom:5px;}
.intro .tag a{ padding-left:15px; color:#204c89}
.new_btm{ height:21px;}

/*产品列表*/
.comBox .cg{ padding:0 10px;font-size:20px; height:50px; line-height:50px; background-color:#f7f7f7;}
.comBox .cg  strong{ padding-right:10px;}
.comBox .cg a{ color:#434343; font-size:18px;}
.comBox .cg em{ font-style:normal; color:#e2e2e2; padding:0 10px; font-size:16px; vertical-align:top;}

.listTop{ background:none; border:0; border-bottom:3px solid #e9e9e9;}
.page_add{ margin-right:170px;_display:inline}
.s_mleft{ width:755px;}
.plist ul li div.r .info em{ font-size:12px; font-style:normal}

/*news_detail*/
.font08 { color:#666; font-size:12px;}
.listleft02_1 { float: left; width:753px; height: auto; overflow: hidden; border: 1px solid #92C3F2; background: url(../images/list-line.gif) top repeat-x; }
.listtitle01_1 { float: left; width: 100%; height: 40px; padding-top: 15px; border-bottom: 1px dashed #B7B7B7; text-align: center; margin-bottom: 5px; }
.listtitle01_1 h1 { font-size:20px; color:#204c89; font-family:"Microsoft Yahei"; font-weight:normal;}
.listtitle03_2 { float: left; width: 100%; height: 25px; line-height: 25px; font-size: 14px; color: #204C89; text-align: center; margin-bottom: 5px; }
.detail { clear: both; margin: 0 auto; width: 95%; overflow: hidden; line-height: 28px; font-size: 14px; color: #2F2F2F; }
.detail p { margin-bottom: 15px; }
.detail br { margin-bottom: 15px; }
.detail_new { clear: both; margin: 0 auto; width: 95%; overflow: hidden; line-height: 23px; font-size: 14px; color: #2F2F2F; }
.detail_new p { margin-bottom: 15px; }
.detailimg { float: left; width: 525px; overflow: hidden; line-height: 33px; font-size: 14px; color: #2F2F2F; text-align: center }
.listfoot01 { float: left; width: auto; height: 30px; font-size: 12px; line-height: 30px; color: #2F2F2F; text-align: center; float: right; margin-right: 20px; margin-top: 10px; }
.detail_news_keywords { line-height: 30px; width: 600px; float: left; margin: 5px 0px; overflow: hidden; }
.listfoot02 { float: left; width:738px; height: 30px; font-size: 12px; line-height: 30px; color: #2F2F2F; text-align: right; padding-right: 15px; border-top: 1px dashed #B7B7B7; border-bottom: 1px dashed #B7B7B7; background: #F5F8FD; }
.redian-title00 { float: left; width: 100%; height: 23px; background: url(../images/nb_new.gif); border-bottom: 1px solid #92C3F2; border-top: 1px solid #92C3F2 }
.title3 { float: left; width: 130px; height: 15px; margin-left: 20px; line-height: 15px; padding-top: 7px; font-weight: bold; font-size: 14px; color: #000000; text-align: left; display: inline; }
.listnews001 { float: left; width:628px; height: 180px; margin: 10px 0 10px 18px; font-size: 12px; display: inline; }
ul.qushitext { margin: 0px; padding: 0px; }
li.qushitext1 { float: left; width: 366px; margin-top: 4px; margin-left: 4px; height: 21px; line-height: 21px; font-size: 14px; }
li.listnews006_ads { float: left; width: 314px; margin-top: 2px; height: 21px; line-height: 21px; font-size: 14px; text-align: left; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; }

.s_text,.s_text p { line-height:1.8;}


