@charset "utf-8";
/* CSS Document */
.fy_body{ font-family:微软雅黑;}
.fy_top{ background:#002333; height:73px;}
.fy_logo{ float:left; width:170px;}
.fy_nav{ float:right; width:750px; margin-top:20px;}
.fy_nav li{ float:left; line-height:32px; text-align:center; margin:0 5px;}
.fy_nav li a{ display:block; padding:0 10px; color:#fff;}
.fy_nav li.cur a,.fy_nav li a:hover{ background:#fff; border-radius:5px; color:#3a3a3a;}

.fy_banner{ height:457px; overflow:hidden; width:100%;}
.fy_banner .bd li{ height:457px; width:100%; background-repeat:no-repeat; background-position:top center; background-image:url(../images/banner_01.jpg);}

.fymain_bg{ background:#dedede; padding-bottom:30px;}

.fy_mval{ height:360px; background-color:#fff; margin-top:15px; background-repeat:no-repeat; position:relative;}
.mval_left .fypro_list{ left:30px; top:35px;}
.mval_left .ftpro_sum{ left:620px; top:45px;}
.mval_right .fypro_list{ right:30px; top:35px;}
.mval_right .ftpro_sum{ left:35px; top:50px;}

.fypro_list{ width:570px; position:absolute;}
.fypro_list li{ float:left; width:258px; float:left; margin-right:25px; border:1px solid #eee; padding-bottom:15px; box-shadow: 3px 4px 4px #ddd; background:#fff;}
.fypro_list li .fy_img{ width:100%; height:182px; margin-bottom:5px;}
.fypro_list li .fy_img img{ width:100%; height:100%;}
.fypro_list li .fy_name{font-size:14px; font-weight:bold; margin-bottom:5px; }
.fypro_list li .fy_name p{line-height:22px; width:100%;white-space:nowrap;text-overflow:ellipsis;overflow:hidden; text-indent:10px;}
.fypro_list li .fy_name a{ color:#686868; }
.fypro_list li .fy_dg a{ display:block; width:100px; height:26px; line-height:26px; color:#fff; text-align:center; background:url(../images/btn_bg.gif) repeat-x 0 0; float:right; margin-right:15px;}

.ftpro_sum{ position:absolute; font-family:微软雅黑;}
.ftpro_sum .fy_t1{ font-size:26px; font-weight:bold; color:#2b4286;}
.ftpro_sum .fy_t2{ font-size:16px; color:#1b1b1b; margin:5px 0; font-weight:bold;}
.ftpro_sum .fy_more{ width:70px; height:20px; line-height:20px; text-align:center; color:#fff; background:#2b4286; border-top:1px solid #aab3ce;}

.bg_p1{ background-image:url(../images/bg_p1.jpg); background-position:center right;}
.bg_p2{ background-image:url(../images/bg_p2.jpg); background-position:center left;}
.bg_p3{ background-image:url(../images/bg_p3.jpg); background-position:center right;}
.bg_p4{ background-image:url(../images/bg_p4.jpg); background-position:center left;}
.bg_p5{ background-image:url(../images/bg_p5.jpg); background-position:center right;}
.bg_p6{ background-image:url(../images/bg_p6.jpg); background-position:center left;}
.bg_p7{ background-image:url(../images/bg_p7.jpg); background-position:center right;}
.bg_p2.mval_right .ftpro_sum{left:170px; top:120px;}

.fy_about{ padding:20px; background:#fff; margin-top:15px;}
.fy_channel_t{ font-size:24px; font-weight:bold; color:#2b4286; border-bottom:1px solid #ddd; margin-bottom:10px; padding-bottom:5px;}
.fy_channel_t a{ color:#2b4286; }

.fy_listpro{ width:100%; overflow:hidden; padding:10px 0;}
.fy_listpro .fypro_list{ position:relative; width:110%;}
.fy_listpro .fypro_list li{ width:220px; margin-right:15px; margin-bottom:15px;}
.fy_listpro .fypro_list li .fy_img{ height:155px;}

/*产品订单*/
.order_list{ padding-top:10px;}
.list08 { background:#f8f8f8;}
.title08 { width:100%; height:30px; line-height:30px; float:left; border-bottom:#fff 1px solid;}
.title08 h3 { width:auto; float:left; font-size:12px; font-weight:normal; padding-left:15px; color:black;}
.title08 h3 b { color:#075cac; font-size:14px;}
.title08 span { width:auto; float:right; padding-right:12px; font-family:Verdana;}
.title08 span font { color:#ff6600; font-weight:bold; font-family:Verdana;}

.list08 ul.ti { width:100%; height:30px; float:left; border-bottom:#ededed 1px solid; background:#f0f0f0;}
.list08 ul.ti li { height:30px; line-height:30px; float:left; text-align:center; font-weight:bold; border-left:#fff 1px solid; border-right:#dadada 1px solid;}
.list08 ul.ti li.i1 { width:50px;}
.list08 ul.ti li.i2 { width:250px;}
.list08 ul.ti li.i3 { width:140px;}
.list08 ul.ti li.i4 { width:107px;}
.list08 ul.ti li.i5 { width:100px;}
.list08 ul.ti li.i6 { width:146px;}
.list08 ul.ti li.i7 { width:130px;}



.list08 div.li { width:100%; float:left; margin-bottom:10px;}
.list08 div.li ul { width:100%; height:30px; line-height:30px; float:left; border-bottom:#ededed 1px solid;}
.list08 div.li ul.libg01 { background:#f8f8f8;}
.list08 div.li ul.libg02 { background:#e7eef7;}
.list08 div.li ul li { float:left; height:30px; line-height:30px; border-left:#fff 1px solid; border-right:#ededed 1px solid; text-align:center; white-space:nowrap; text-overflow:ellipsis; overflow:hidden;}
.list08 div.li ul li.i1,.list08 div.li ul li.i7 { font-size:11px; font-family:Verdana;}

.list08 div.li ul li.i1 { width:50px;}
.list08 div.li ul li.i2 { width:245px;}
.list08 div.li ul li.i3 { width:140px;}
.list08 div.li ul li.i4 { width:107px;}
.list08 div.li ul li.i5 { width:100px;}
.list08 div.li ul li.i6 { width:146px;}
.list08 div.li ul li.i7 { width:130px;}

.order_title { width:100%; float:left; border-bottom:#e0e0e0 1px solid;}
.order_title h3 { width:auto; float:left; font:700 14px/25px Arial; color:#333; padding-left:10px;}
.order_title span.more { width:auto; float:right;  line-height:25px; padding-right:10px;}
.border03 { border:#cccccc 1px solid;}


.fy_contact{ background:url(../images/conbg.gif) no-repeat 286px 15px; padding:20px 10px 30px;}
.fy_contact_nr{ margin-top:5px;}
.fy_contact_nr p{ padding-left:50px;}