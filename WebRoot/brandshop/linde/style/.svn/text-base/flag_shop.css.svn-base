/* CSS Document */
.slide { position: relative; width:100%; }
.ban_c { display:none;position: absolute; left: 0; top: 0; width: 100%; height: 100%; overflow: hidden; }
.ban_c img { width: 100%; height: 100%; }
.ban_nav { position: absolute; bottom: 0; right:0;font-size:0;z-index: 4; }
.ban_nav a { display: inline-block; vertical-align: middle; width: 14px; height: 14px; line-height: 14px; overflow: hidden; text-align: center; margin-right: 8px; cursor: pointer; text-decoration: none; font-size: 10px; -webkit-text-size-adjust: none; background: #333; color:#fff;border-radius:50%;}
.ban_nav .on { background: #3789ef; color: #fff; }
.Left, .Right { cursor: pointer; }

body { font-family:"Microsoft Yahei"; background:url(../images/shopbg.png) repeat;}
.shop_topbg { width:100%; height:109px; clear:both; background:url(../images/topbg.jpg) center top no-repeat;}
.shop_topbg h2 { width:auto; float:left;}
.shop_bm { width:auto; height:auto; float:right; margin:4px 23px 0px 0px; _display:inline;}
.shop_tel { width:160px; float:right; font-size:16px; line-height:22px; color:#fff; padding-top:14px;}

.shopNav li { width:auto; height:45px; float:left;}
.shopNav li a { display:inline-block; width:auto; height:45px; line-height:44px; font-size:16px; color:#fff; font-weight:bold; position:relative; padding:0px 35px; overflow:hidden;}
.shopNav li a i { display:none; width:24px; height:45px; position:absolute; right:0px; top:0px; background:url(../images/nav_selected.png) right no-repeat;}
.shopNav li a:hover,.shopNav li a.selected { background:url(../images/nav_selected.png) no-repeat;}
.shopNav li a:hover i,.shopNav li a.selected i { display:block;}

.shopBanner { height:387px; overflow:hidden;}
.shopBanner .ban_nav { right:50%; margin-right:-475px; bottom:10px;}
.shopBanner .ban_nav a { width:16px; height:16px; background-color:#d9d9d9; font:0/0 a; letter-spacing:-9px;}
.shopBanner .ban_nav a.on { background-color:#d60000;}
.shopBanner .ban_c img { width:1920px; height:387px; position:absolute; left:50%; top:0px; margin-left:-960px;}

.shopWrap { width:980px; background-color:#fff; padding-left:10px; padding-right:10px; clear:both; margin-left:auto; margin-right:auto;}

.recPro { position:relative;}
.xj_btn { width:auto; height:68px; line-height:66px; color:#fff!important; font-size:36px; font-weight:bold; background-color:#e30000; padding:0px 16px; position:absolute; left:75px; top:315px;}

.about_intro { padding-top:1px; padding-bottom:10px;}
.about_intro .box { background-color:#717171;}
.about_intro .l { border-right:#fff 1px solid; margin-right:30px;}
.about_intro .text { padding-top:20px; line-height:24px; color:#fff; padding-right:30px;}

.shopWrap .hd { height:87px; background:url(../images/tbg.jpg) no-repeat; margin-bottom:10px; position:relative; cursor:default;}
.shopWrap .hd h3 { font-size:38px; font-weight:bold; color:#fff; line-height:85px; padding-left:23px;}
.shopWrap .hd .more { width:85px; height:30px; line-height:30px; text-align:center; background:#e30000; color:#fff; position:absolute; right:0px; bottom:0px;}
.shopWrap .hd .more a { color:#fff!important; display:block;}
.proList { width:100%; overflow:hidden; position:relative;}
.proList ul { width:110%; *padding-bottom:10px;}
.proList li { width:483px; height:390px; border:#434343 1px solid; float:left; margin:0px 10px 10px 0px; position:relative;}
.proList li .ico { width:132px; height:133px; background:url(../images/pro_ico.png) no-repeat; position:absolute; left:0px; top:0px;}
.proList img { display:block; width:400px; height:300px; margin:0 auto;}
.proList .intro { width:100%; height:90px; background-color:#d5d5d5; position:relative;}
.proList .btn { display:block; width:85px; height:81px; position:absolute; right:5px; top:4px; background:url(../images/xj_btn.png) no-repeat; font:0/0 a; letter-spacing:-9px;} 
.proList .intro h3 { font-size:16px; width:360px; height:26px; line-height:26px; word-break:break-all; overflow:hidden; padding:20px 0px 0px 15px;}
.proList .intro .otLink { display:block; height:24px; line-height:24px; padding-left:15px;}
.proList .intro .otLink em { font-style:normal; margin:0px 8px;}

.proOrder .content { height:374px; background-color:#fff;}
.proOrder .i2 { width:293px;}
.proOrder .list08 div.li ul li.i2 { width:288px;}
.proOrder .i3 { width:140px;}
.proOrder .i6 { width:146px;}

.proOrder .list08 div.li { margin-bottom:0px;}

.proOrder .New_website { display:none;}
.proOrder .New_foot { margin-top:15px;}

.order_title { width:100%; float:left; border-bottom:#e0e0e0 1px solid;}
.order_title h3 { width:auto; float:left; font:700 14px/25px Arial; color:#333; padding-left:10px;}
.order_title span.more { width:auto; float:right; font-family:宋体; line-height:25px; padding-right:10px;}

.bottombg { background:#fff; clear:both; padding-top:10px; margin-top:20px;}
.bottombg .New_website { margin-top:0;}

.channelBox { width:960px; padding:20px 20px; margin-top:15px; margin-top:20px;}

.channelLeft { width:205px; float:left;}
.channelRight { width:730px; float:right;}

.channelLeft .hd { height:55px; line-height:40px; font-size:22px; background:none; color:#000; margin-bottom:0;}
.shopListMenu { margin-bottom:15px;}
.shopListMenu li { padding-bottom:1px;}
.shopListMenu li a { display:block; width:183px; height:39px; padding-left:20px; background:url(../images/menubg.png) 0px 0px no-repeat; font:700 14px/38px Arial; color:#1f1f1f!important; border:#e0e4eb 1px solid;}
.shopListMenu li a:hover,.shopListMenu li.select a { background:#d60000; color:#fff!important; border-color:#d60000;}

.channelLeft .box { clear:both;}
.channelLeft .box h3 { height:40px; line-height:40px; font-size:14px; font-weight:bold; padding-left:20px; background:#d60000; color:#fff;}
.channelLeft .intro { border:#d2d2d2 1px solid; border-top:none; padding:15px;}
.channelLeft .intro strong.t { font-weight:normal; color:#d60000;}
.channelLeft .intro p { line-height:2;}

.channelRight .hd { height:55px; line-height:40px; font-size:22px; background:none; color:#000; margin-bottom:0;}
.channelRight .hd strong { width:auto; height:42px; float:left; padding-right:110px; background:url(../images/list_ld_t.png) right no-repeat;}

.proList01 { width:100%; overflow:hidden;}
.proList01 ul { width:110%; padding-bottom:0px; *padding-bottom:20px;}
.proList01 li { width:225px; height:260px; float:left; border:#cfcfcf 1px solid; background-color:#fff; margin:0px 22px 20px 0px;}
.proList01 li .img { width:100%; float:left; padding:19px 0px;}
.proList01 li .img img { width:225px; height:174px;}
.proList01 li h3 { width:215px; height:20px; padding-left:10px; float:left; font:100 12px/20px Arial; background-color:#f3f3f3; padding-top:5px;}
.proList01 li a { color:#555555!important;}
.proList01 li .links { width:150px; height:20px; padding-left:10px; float:left; font:100 12px/20px Arial; background-color:#f3f3f3; padding-bottom:3px;}
.proList01 li a.priceBtn { width:65px; height:20px; font:700 12px/20px 微软雅黑; background-color:#f3f3f3; float:right; color:#bf0707!important; padding-bottom:3px;}
.proList01 li.hover { border-color:#174eea;}
.proList01 li.hover h3,.shopPros01 li.hover .links,.shopPros01 li.hover a.priceBtn { background-color:#174eea; color:#fff;}
.proList01 li.hover a { color:#fff!important;}





