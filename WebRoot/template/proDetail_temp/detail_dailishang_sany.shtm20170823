<script> 
var oXjBox=document.getElementById('xjbox');
var oShowCarCompare=oCom_wrap=document.getElementById('divShowCarCompare');//对比插件
var oCloseBox=document.getElementById('closebox');
var oXjbox_display=document.getElementById('xjbox_display');
var top_xj=document.getElementById('top_xj');
var viewHeight=document.documentElement.clientHeight;
var viewWidth=document.documentElement.clientWidth;

//询价浮动
var h=$('.pro_top_hd_box').offset().top;
   $(window).scroll(function(){
	   var scrolls=$(window).scrollTop();
	   if(scrolls>h){
		   $('#xjbox').addClass('xjbox_');
		   }else{
			   $('#xjbox').removeClass('xjbox_');
			   }
	});
	
//对比浮动
$(window).scroll(function(){
	 var scrolls=$(window).scrollTop();
	 if(scrolls>h){
		 $('#divShowCarCompare').addClass('fipop_cont_');//对比插件
		 }else{
			 $('#divShowCarCompare').removeClass('fipop_cont_');//对比插件
			 }
	 });


  $('#xjbox').css('width',function(){
	  return (viewWidth-1200)/2+285;
	  });
  $('#cmpContainer').css('width',function(){
	  return (viewWidth-1200)/2+285;
	  });
  $('#divShowCarCompare').css('width',function(){
	  return (viewWidth-1200)/2+285;
	  });
  $('.empty').css('margin-left',function(){
	  return $('#cmpContainer').width()/2+85;
	  });
  $('#xjbox').find('button').css('margin-left',function(){
	  return ($('#xjbox').width()-200)/2;
	  })


  function CloseBox(){
	  oXjBox.style.height=(viewHeight-31)+'px';
	  oShowCarCompare.style.height=(viewHeight-31)+'px';//对比插件
	  oCloseBox.onclick=function(){  
		oXjBox.style.display='none';
	    oXjBox.style.right=-600+'px';  
	  }
	  
	  oXjbox_display.onclick=function(){
		  oXjBox.style.display='block';
		  startMove(oXjBox,{right:0});
		  oXjBox.style.position='fixed';
		  if(oCom_wrap.style.display=='block'){
			  $('#divShowCarCompare').css('display','none');
	          startMove(oCom_wrap,{right:-600});
			  }
	  }
	  
	  top_xj.onclick=function(){
		  oXjBox.style.display='block';
		  startMove(oXjBox,{right:0});
		  oXjBox.style.position='fixed';
		  if(oCom_wrap.style.display=='block'){
			  $('#divShowCarCompare').css('display','none');
	          startMove(oCom_wrap,{right:-600});
			  }
	  }
  }
  CloseBox();
  
   //对比插件
 $('.fiopen').click(function(){
	 if($('#xjbox').css('display','block')){
		 $('#xjbox').css('display','none');
		 $('#xjbox').css('right','-600px');
		 }
	 $(this).css('display','none');
	 $('#divShowCarCompare').css('display','block');
	 startMove(oCom_wrap,{right:0});
	 $('#divShowCarCompare').css('position','fixed');
	 });
 $('.fiaway').click(function(){
	 $('#divShowCarCompare').css('display','none');
	 startMove(oCom_wrap,{right:-600});
	 $('.fiopen').css('display','block');
	 $.cookies.set("compareOpenFlag", "close");
	 });
 //插件结束  
</script>
<!--询价对比结束-->
</body>
