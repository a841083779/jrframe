<script>
  	  function tj3(){ 
		  if(jQuery('#keyword').val()==''){
		     alert("请输入您要搜索的关键词");
		     jQuery('#keyword').focus();
			 return false;
		  }
		  var keyword=jQuery('#keyword').val();
		  window.open("http://product.21-sun.com/search/"+keyword+"_0_0_0.htm");
	  }
	   $('#keyword').on('keydown', function (e) { if (e.keyCode == 13) {tj3()} })

	   
	   
// 把下过订单的信息取出
jQuery(function(){
	var order_name = jQuery.trim($.cookies.get("order_name"));  // 订单人
    var order_phone = jQuery.trim($.cookies.get("order_phone")); ;  // 订单电话
    if(''!=order_name && null!=order_name && "null"!=order_name){
    	jQuery("#zd_name").val(order_name) ;
    }
    if(''!=order_phone && null!=order_phone && "null"!=order_phone){
    	jQuery("#zd_mobile_phone").val(order_phone) ;
    }
})

</script>
