<#assign provinceList = query(" select  * from hz_py where province='0' ")>
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<#list provinceList as province>
		<#list query(" select  * from hz_py where province='${province.hanzi}' ") as city>
  
			<#list query(" select top 100 id from pro_agent_factory where id not in (select top 300 id from pro_agent_factory where is_show=1 and flag=1 order by id asc) and  is_show=1 and flag=1 order by id asc") as factory>
				    <url>
                        <loc>http://product.21-sun.com/agent/${province.pinyin}_${city.pinyin}_${factory.id?c}_0.shtm</loc>
                        <lastmod>2014-06-11</lastmod>
						<priority>1.0</priority>
                        <changefreq>monthly</changefreq>
                    </url>
			</#list>   
	    </#list>	
	</#list>	
</urlset>