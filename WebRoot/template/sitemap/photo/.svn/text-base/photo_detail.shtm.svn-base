<#assign products_list = query(" select * from pro_products where is_show = 1 ")>
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<#list products_list as products>
				<url>
					<loc>http://photo.21-sun.com/detail_for_${products.id?c}.htm</loc>
            				<lastmod>${.now}</lastmod>
					<priority>1.0</priority>
					<changefreq>monthly</changefreq>
				</url>
	</#list>
</urlset>