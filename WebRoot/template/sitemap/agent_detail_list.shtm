<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<#list proList as pro>  
        <url>
            <loc>http://product.21-sun.com/agent/${pro.usern}/</loc>
			<lastmod>2014-06-11</lastmod>
            <priority>1.0</priority>
            <changefreq>monthly</changefreq>
        </url>
		<url>
            <loc>http://product.21-sun.com/agent/${pro.usern}/introduce.htm</loc>
