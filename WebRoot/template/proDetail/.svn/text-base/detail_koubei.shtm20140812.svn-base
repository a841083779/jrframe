<script>

jQuery('#name').val(remote_ip_info['province']+remote_ip_info['city']+'的网友');
	//浏览次数增加、返回
	jQuery.ajax({
		url:"/action/ajax.jsp",
		type:"post",
		data:{"id":"${model.id?c}","flag":"viewCount","tableName":"pro_products"},
		success:function(data){
			jQuery("#viewCountId").html(jQuery.trim(data));	
		}
	})

	
	//获取产品得分和星星 
		function getAllComment(){
		jQuery.getJSON("/action/ajax.jsp?flag=get_all_comment&callback=?&pid=${model.id?c}",function(data){
			var s1 = (parseInt(data.score1,10)/20);
			var s2 = (parseInt(data.score2,10)/20);
			var s3 = (parseInt(data.score3,10)/20);
			var s4 = (parseInt(data.score4,10)/20);
			var sall = ((s1*5)+(s2*5)+(s3*5)+(s4*5));
			jQuery("#totalScoreId").html(sall);
			jQuery("#pricescore").raty({
				half : true ,
			    readOnly : true,
				score : s1
			});
			jQuery("#abilityscore").raty({
				half : true ,
				readOnly : true,
				score : s2
			});
			jQuery("#qualityscore").raty({
				half : true ,
				readOnly : true,
				score : s3
			});
			jQuery("#servicescore").raty({
				half : true ,
				readOnly : true,
				score : s4
			});
		});
	}
	getAllComment();
    //评论列表
	jQuery.ajax({
		url:"/action/ajax_list.jsp",
		type:"post",
		data:{"id":"${model.id?c}","flag":"commentListNew","name":"${model.name}","factoryname":"${model.factoryname}","catalogname":"${model.catalogname}"},
		success:function(data){
			jQuery("#commentListId").html(jQuery.trim(data));	
		}
	})

    //评论提交	
	jQuery("#price_1,#xingneng_1,#zhiliang_1,#shouhou_1").raty({
		half : true ,
		score : 4
	});
	function doComments(){
		var rs = Validator.Validate(document.getElementById("comment_form"),1);
		if(rs){
		   var  content = jQuery.trim(jQuery("#content").val()) ;
		   if(content!=content.replace(/<\/?[^>]*>/g,'').replace('&lt','').replace('&gt','') || (content.indexOf("www")!=-1 || content.indexOf("http")!=-1)){
		       alert('您的评论内容不符合规则，请重新输入!') ;	
		       jQuery("#content").val("") ;
		       jQuery("#content").focus() ;
		        return false ;
		   }
			jQuery("#score1").val(parseFloat(jQuery("#price_1").raty("score"),10)*20);
			jQuery("#score2").val(parseFloat(jQuery("#xingneng_1").raty("score"),10)*20);
			jQuery("#score3").val(parseFloat(jQuery("#zhiliang_1").raty("score"),10)*20);
			jQuery("#score4").val(parseFloat(jQuery("#shouhou_1").raty("score"),10)*20);
			jQuery("#comment_form").ajaxSubmit({
				type : "POST",
				async : false ,
				success : function(data){
			     if(jQuery.trim(data)=='no'){
				   alert("您输入的评价中含有敏感词，请重新输入") ;
				    jQuery("#content").val("") ;
		            jQuery("#content").focus() ; 
				   return ;
				}
					alert("评论提交成功！");
					//getComment();
					jQuery("#comment_form").clearForm();
					jQuery("#price_1,#xingneng_1,#zhiliang_1,#shouhou_1").raty({
						half : true ,
						score : 4
					});
				}					  
			});
		}
	}
		//栏目提示
	function ts(){
	 $.jBox.tip("栏目资料完善中，敬请期待！");
	}
</script>

